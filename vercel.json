{
  "buildCommand": "npm install",
  "devCommand": "node api/server.js",
  "outputDirectory": "./",
  "functions": {
    "api/**/*.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "env": {
    "DATABASE_URL": "@DATABASE_URL",
    "ADMIN_USER": "@ADMIN_USER",
    "ADMIN_PASS": "@ADMIN_PASS",
    "RESET_CONFIRM": "@RESET_CONFIRM",
    "TARGET_COUNT": "@TARGET_COUNT",
    "NODE_ENV": "production"
  }
}
